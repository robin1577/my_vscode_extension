{
    "name": "cobalt-strike-aggressor-script",
    "displayName": "cs脚本语言",
    "description": "cs脚本语言扩展",
    "keywords": [
        "cna",
        "cobalt",
        "cs"
    ],
    "version": "1.0.0",
    "homepage": "https://github.com/Twi1ight/vscode-language-aggressorscript",
    "license": "BSD-3-Clause",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.65.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Twi1ight/vscode-language-aggressorscript.git"
    },
    "publisher": "Twi1ight",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:cna"
    ],
    "main": "./lsp/client/out/extension",
    "contributes": {
        "languages": [
            {
                "id": "cna",
                "aliases": [
                    "Aggressor Script",
                    "cna"
                ],
                "extensions": [
                    ".cna"
                ],
                "configuration": "./language_base/language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cna",
                "scopeName": "source.cna",
                "path": "./language_base/syntaxes/cna.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "cna",
                "path": "./language_base/snippets/cna.json"
            }
        ]
    },
    "author": {
        "name": "Twi1ight",
        "url": "https://github.com/Twi1ight/"
    },
    "credits": [
        {
            "name": "Carlos Perez",
            "url": "https://www.darkoperator.com/"
        },
        {
            "name": "Paul Laîné",
            "url": "https://twitter.com/am0nsec"
        }
    ]
}
